      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/orders/:id/reject",
      "handler": "order.reject",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/orders/:id/cancel",
      "handler": "order.cancel",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/orders/:id",
      "handler": "order.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/orders/:id",
